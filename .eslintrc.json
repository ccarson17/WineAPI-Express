{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "import/extensions": 0,
        "eqeqeq": "off",
        "comma-dangle": 0,
        "import/no-extraneous-dependencies": 0,
        "no-unused-vars": 0
    }
}
